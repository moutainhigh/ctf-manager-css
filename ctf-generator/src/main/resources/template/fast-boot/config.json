{
  "project": {
    "packageName": "com.ctf",
    "version": "1.0.0",
    "backendPath": "D:\\generator\\ctf-backend",
    "frontendPath": "D:\\generator\\ctf-frontend"
  },
  "developer": {
    "author": "H.m",
    "email": "hackmgod@foxmail.com"
  },
  "templates": [
    {
      "templateName": "Controller.java.ftl",
      "generatorPath": "${backendPath}/src/main/java/${packagePath}/${moduleName!}/controller/${subModuleName!}/${ClassName}Controller.java"
    },
    {
      "templateName": "Service.java.ftl",
      "generatorPath": "${backendPath}/src/main/java/${packagePath}/${moduleName!}/service/${subModuleName!}/${ClassName}Service.java"
    },
    {
      "templateName": "ServiceImpl.java.ftl",
      "generatorPath": "${backendPath}/src/main/java/${packagePath}/${moduleName!}/service/impl/${subModuleName!}/${ClassName}ServiceImpl.java"
    },
    {
      "templateName": "Query.java.ftl",
      "generatorPath": "${backendPath}/src/main/java/${packagePath}/${moduleName!}/query/${subModuleName!}/${ClassName}Query.java"
    },
    {
      "templateName": "Entity.java.ftl",
      "generatorPath": "${backendPath}/src/main/java/${packagePath}/${moduleName!}/entity/${subModuleName!}/${ClassName}Entity.java"
    },
    {
      "templateName": "VO.java.ftl",
      "generatorPath": "${backendPath}/src/main/java/${packagePath}/${moduleName!}/vo/${subModuleName!}/${ClassName}VO.java"
    },
    {
      "templateName": "Convert.java.ftl",
      "generatorPath": "${backendPath}/src/main/java/${packagePath}/${moduleName!}/convert/${subModuleName!}/${ClassName}Convert.java"
    },
    {
      "templateName": "Mapper.java.ftl",
      "generatorPath": "${backendPath}/src/main/java/${packagePath}/${moduleName!}/dao/${subModuleName!}/${ClassName}Mapper.java"
    },
    {
      "templateName": "Mapper.xml.ftl",
      "generatorPath": "${backendPath}/src/main/resources/mapper/${moduleName!}/${ClassName}Mapper.xml"
    },
    {
      "templateName": "menu.sql.ftl",
      "generatorPath": "${backendPath}/menu.sql"
    },
    {
      "templateName": "api.ts.ftl",
      "generatorPath": "${frontendPath}/src/api/${moduleName!}/${classname}.ts"
    },
    {
      "templateName": "index.vue.ftl",
      "generatorPath": "${frontendPath}/src/views/${moduleName!}/${classname}/index.vue"
    },
    {
      "templateName": "add-or-update.vue.ftl",
      "generatorPath": "${frontendPath}/src/views/${moduleName!}/${classname}/add-or-update.vue"
    }
  ]
}
